{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    { "src": "package.json", "use": "@vercel/node" },
    { "src": "storybook-static/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/storybook", "dest": "/storybook-static" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
